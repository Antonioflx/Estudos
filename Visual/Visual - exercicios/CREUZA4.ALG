Algoritmo "Creuza4"

Var
   Valor,Resultado:real


Inicio
      Escreva("A senhora gastou quanto nesse equipamento? ")
      Leia(Valor)
      Escreval("A taxação está de 60%")
      Escreval("Logo, ficará o valor do seu produto multiplicado por 60 por cento")
      Resultado <- (Valor * 60)/100
      Escreva("Então, o imposto foi de: ", Resultado:5:2, " reais ")

Fimalgoritmo